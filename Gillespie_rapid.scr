#!/bin/bash

rm -f kaiRustl.0*
rm -f *~
rm -f avr*

Gillespie_parse $1 > Gillespie.inp
Gillespie

# Kondo model.
#ls *lCp* | Gillespie_add avr.lCp
#ls *lATP* | Gillespie_add avr.lATP
